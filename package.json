{
  "name": "ssr",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node scripts/start.js --dev",
    "tsnd": "ts-node-dev --no-notify src/server.ts",
    "tsnode": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/server.ts NODE_ENV=development",
    "tsnode:debug": "nodemon --inspect --watch 'src/**/*' -e ts,tsx --exec 'node -r ts-node/register' src/server.ts",
    "build": "yarn build:client && yarn build:server",
    "build:client": "webpack --config ./config/client/webpack.config.js --prod",
    "build:server": "webpack --config ./config/server/webpack.config.js --prod",
    "build:client2": "node scripts/build_client.js --prod",
    "build:server2": "node scripts/build_server.js --prod",
    "prod": "node ./build/server/main.js"
  },
  "dependencies": {
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.37",
    "@types/koa-static": "^4.0.0",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.8",
    "@types/webpack": "^4.4.25",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-modules-require-hook": "^4.2.3",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "isomorphic-style-loader": "^5.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "react-dev-utils": "^11.0.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tsconfig-paths": "^3.7.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.2.2",
    "typescript-plugin-styled-components": "^1.4.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
